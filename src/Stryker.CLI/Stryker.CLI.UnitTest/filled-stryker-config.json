{
  "stryker-config": {
    "reporters": [
      "Json"
    ],
    "verbosity": "trace",
    "coverage-analysis": "perTest",
    "disable-bail": true,
    "disable-mix-mutants":  true,
    "additional-timeout": 9999,
    "project": "ExampleProject.csproj",
    "concurrency": 1,
    "solution": "",
    "mutation-level": "",
    "since": {
      "enabled": false,
      "target": "main",
      "ignore-changes-in": [
        ""
      ]
    },
    "language-version": "",
    "dashboard-url": "",
    "test-projects": [
      ""
    ],
    "ignore-mutations": [
      "linq.FirstOrDefault",
    ],
    "ignore-methods": [
      ""
    ],
    "thresholds": {
      "high": 40,
      "low": 30,
      "break": 20
    },
    "mutate": [
      "!**/Test.cs{1..100}{200..300}"
    ],
    "project-info": {
      "name": "github.com/stryker-mutator/stryker-net",
      "version": "master",
      "module": "cli"
    },
    "baseline": {
      "enabled":  false,
      "fallback-version": "master",
      "azure-fileshare-url": "",
      "provider": ""
    }
  }
}
