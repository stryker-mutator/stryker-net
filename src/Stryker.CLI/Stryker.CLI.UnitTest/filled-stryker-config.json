{
  "stryker-config": {
    "reporters": [
      "ConsoleReport"
    ],
    "log-level": "trace",
    "dev-mode": true,
    "coverage-analysis": "perTest",
    "abort-test-on-fail": true,
    "timeout-ms": 9999,
    "log-file": true,
    "project-file": "ExampleProject.csproj",
    "max-concurrent-test-runners": 1,
    "threshold-high": 40,
    "threshold-low": 30,
    "threshold-break": 20,
    "mutate": [
      "!**/Test.cs{1..100}{200..300}"
    ],
    "mutations-options": [
      "!linq.firstordefault"
    ]
  }
}
