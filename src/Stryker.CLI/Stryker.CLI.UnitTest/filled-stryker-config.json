{
  "stryker-config": {
    "reporters": [
      "ConsoleReport"
    ],
    "log-level": "trace",
    "dev-mode": true,
    "coverageAnalysis": "perTest",
    "advancedOptions": [
      "abortTestOnFail"
    ], 
    "timeout-ms": 9999,
    "log-file": true,
    "project-file": "ExampleProject.csproj",
    "max-concurrent-test-runners": 1,
    "threshold-high": 40,
    "threshold-low": 30,
    "threshold-break": 20,
    "files-to-exclude": [
      "./Recursive.cs"
    ] 
  }
}