{
  "stryker-config": {
    "reporter": "ReportOnly",
    "log-level": "trace",
    "timeout-ms": 9999,
    "log-file": true,
    "project-file": "ExampleProject.csproj",
    "max-concurrent-test-runners": 10,
    "threshold-options": {
      "threshold-high": 40,
      "threshold-low": 30,
      "threshold-break": 20
    },
    "files-to-exclude": [
      "./Recursive.cs"
    ] 
  }
}